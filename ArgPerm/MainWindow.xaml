<Window x:Class="ArgPerm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArgPerm"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TreeView x:Name="treeView_Directorys"  HorizontalAlignment="Left" Grid.Column="0"/>
        <ListBox x:Name="listBox_test" Grid.Column="1" Margin="0,10,0,0"/>
        <Label x:Name="lbl_time" Content="Label" HorizontalAlignment="Left" Margin="99,383,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
